{
  "name": "fakeminder",
  "version": "0.0.1",
  "description": "Fake or mock implementation of CA SiteMinder to use for developer testing",
  "bin": {
    "fakeminder": "bin/fakeminder"
  },
  "preferGlobal": true,
  "scripts": {
    "run": "clear && node main.js",
    "test": "clear && mocha -R spec",
    "coverage": "istanbul cover _mocha -- -R spec && open coverage/lcov-report/index.html",
    "run-sample": "clear && node ./sample_target/app.js",
    "jshint": "clear && jshint ./lib/*.js main.js ./sample_target/routes/*.js ./sample_target/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisneave/fakeminder.git"
  },
  "keywords": [
    "siteminder"
  ],
  "author": "Chris Neave",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisneave/fakeminder/issues"
  },
  "dependencies": {
    "http-proxy": "0.10.x",
    "cookies": "0.3.x",
    "express": "3.x",
    "ejs": "0.8.x",
    "underscore": "1.5.x",
    "commander": "~2.1.0",
    "inquirer": "~0.3.5"
  },
  "devDependencies": {
    "mocha": "1.x",
    "expect.js": "0.x",
    "cookie": "0.x"
  }
}
